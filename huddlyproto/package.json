{
  "name": "@huddly/huddlyproto",
  "version": "0.0.3",
  "description": "An npm package describing and bundling all the huddly proto files",
  "keywords": [
    "SDK",
    "Huddly",
    "Proto",
    "GRPC"
  ],
  "author": "Huddly AS",
  "homepage": "http://developer.huddly.com",
  "engines": {
    "node": ">=12.4.0"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "build.sh",
    "proto"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huddly/sdk.git"
  },
  "scripts": {
    "build": "bash build.sh && npm run build-ts",
    "build-ts": "tsc --declaration",
    "prepare": "bash build.sh",
    "publish-package": "npm run build && npm run build-ts && npm publish"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Huddly/sdk/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "grpc-tools": "^1.11.1",
    "grpc_tools_node_protoc_ts": "^5.2.2",
    "ts-protoc-gen": "^0.15.0"
  },
  "devDependencies": {
    "typescript": "^4.3.2"
  }
}
