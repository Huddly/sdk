{
  "name": "@huddly/sdk",
  "version": "0.2.12",
  "description": "Huddly's Software Development Kit (SDK) used to interact with the camera by third-party applications",
  "keywords": [
    "SDK",
    "API",
    "Huddly",
    "AI",
    "People",
    "Count",
    "Genius",
    "Framing"
  ],
  "author": "Huddly AS",
  "homepage": "http://developer.huddly.com",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "engines": {
    "node": ">=8.11.2"
  },
  "directories": {
    "lib": "lib",
    "test": "tests",
    "bundle": "bundle"
  },
  "files": [
    "lib",
    "README.md",
    "bundle"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huddly/sdk.git"
  },
  "scripts": {
    "build": "npm run build-ts && npm run tslint && npm run test",
    "build-web": "npm run build-ts && npm run tslint && yarn webpack -p",
    "build-ts": "tsc --declaration",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc mocha -r ts-node/register --recursive \"tests/**/*.spec.ts\" --check-leaks",
    "test-js": "mocha -r ts-node/register --recursive \"lib/tests/**/*.spec.js\"",
    "publish-package": "npm run build && npm run build-web && npm publish",
    "compodoc": "./node_modules/.bin/compodoc -w -p tsconfig.json -s -n 'huddly-sdk' -d 'docs' --minimal --customFavicon 'docs_style/assets/imgs/favicon.ico' --disableGraph --disableInternal --hideGenerator --includesName 'Examples' --includes examples -a docs_style/assets -y docs_style",
    "prepare": "npm run build-ts",
    "preversion": "npm run tslint",
    "prettier": "./node_modules/.bin/prettier  --config ./.prettierrc --write src/**/*.ts",
    "prettier-check": "./node_modules/.bin/prettier  --config ./.prettierrc --check src/**/*.ts",
    "commit": "git-cz"
  },
  "pre-commit": [
    "prettier-check",
    "build"
  ],
  "bugs": {
    "url": "https://github.com/Huddly/sdk/issues"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.8",
    "@types/mocha": "~5.2.5",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "chai-things": "~0.2.0",
    "commitizen": "^3.1.1",
    "cross-env": "~5.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "mocha": "^6.1.4",
    "nyc": "~14.1.1",
    "pre-commit": "~1.2.2",
    "prettier": "^1.17.1",
    "sinon": "^7.3.2",
    "sinon-chai": "~3.3.0",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/node": "~12.0.5",
    "source-map-support": "~0.5.11",
    "jszip": "^3.2.1",
    "msgpack-lite": "~0.1.26",
    "p-queue": "^6.0.1",
    "semver": "^6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
